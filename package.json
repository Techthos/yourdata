{
  "name": "yourdata",
  "version": "1.0.0",
  "main": "index.js",
  "dependencies": {
    "pnpm": "^8.14.1"
  },
  "scripts": {
    "docker:build": "docker build . -t yourdata",
    "publish:packages": "pnpm --filter @yourdata/* publish",
    "build": "pnpm --filter @yourdata/* build",
    "dev": "pnpm --filter @yourdata/* --parallel run dev"
  },
  "engines": {
    "node": "18",
    "pnpm": ">=3"
  }
}
